{
  "expo": {
    "name": "petmigos-app",
    "slug": "petmigos-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#7b4d28"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "fonts": [{
        "fontName": "Ubuntu",
        "fontFile": "./fonts/Ubuntu-Regular.ttf"
    }]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#7b4d28"
      },
      "fonts": [
        "./assets/fonts/Ubuntu.ttf",
        "./assets/fonts/Ubuntu-Bold.ttf",
        "./assets/fonts/Ubuntu-Italic.ttf",
        "./assets/fonts/Ubuntu-Light.ttf",
        "./assets/fonts/Ubuntu-Medium.ttf",
        "./assets/fonts/Ubuntu-Regular.ttf"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}